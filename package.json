{
  "name": "sample-service",
  "version": "1.0.0",
  "description": "sample service with DDD approach",
  "private": "true",
  "scripts": {
    "build": "lerna run build --stream",
    "build:all": "nx run-many -t build",
    "build:sample": "nx build @lkie/sample-data",
    "deploy:sample": "nx deploy @lkie/sample-data",
    "build:craftdocs": "nx build @lkie/craftdocs-impl",
    "deploy:craftdocs": "nx deploy @lkie/sample-impl",
    "shared-model": "lerna run build --stream --scope @lkie/shared-model",
    "aws-commons": "lerna run build --stream --scope @lkie/aws-commons",
    "fix": "eslint --ext .ts --fix ./",
    "test": "jest"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.108",
    "@types/jest": "^29.2.0",
    "@types/node": "20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "axios": "^1.1.3",
    "babel-plugin-transform-util-promisify": "^0.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "envfile": "^6.18.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.1",
    "jest-dynalite": "^3.6.1",
    "jest-junit": "^16.0.0",
    "lerna": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "nx": "16.5.0",
    "nx-cloud": "latest",
    "prettier": "^3.0.0",
    "proxy-agent": "^6.2.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "type-doc": "^0.1.41",
    "typescript": "^5.1.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.1.4"
  },
  "author": "Lukasz Kiedrowski",
  "license": "UNLICENSED",
  "workspaces": {
    "packages": [
      "packages/*/*",
      "services/*/*"
    ],
    "nohoist": [
      "**"
    ]
  }
